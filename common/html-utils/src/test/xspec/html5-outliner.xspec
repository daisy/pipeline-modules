						<h1 id="h12">h12</h1>
					</section>
				</body>
			</html>
		</x:context>
		<x:expect label="outline">
			<ol>
				<li><a href="#body">h11</a>
				<ol>
					<li><a href="#section">h12</a></li>
				</ol>
				</li>
			</ol>
		</x:expect>
	</x:scenario>
	
